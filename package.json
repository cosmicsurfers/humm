{
  "name": "hc-ts-template",
  "version": "0.1.0",
  "description": "",
  "proxy": "http://localhost:4141",
  "private": true,
  "scripts": {
    "hc:build-common": "tsc -p dna-src && cpx \"dna-src/**/*.json\" build/dna",
    "hc:build": "rm -r build/dna; npm run hc:build-common",
    "hc:build-win": "if exist build\\dna\\ cmd /c rd /S /Q build\\dna && npm run hc:build-common",
    "hc:dev": "npm run hc:build && cd build && hcdev web",
    "hc:win": "npm run hc:build-win && cd build && hcdev web",
    "hc:test": "npm run hc:build && rm -r build/test && cp -r ./test/ ./build/ && cd build && hcdev test",
    "ui:build": "rm -r build/ui; node scripts/build.js && cp -r ui-src/build/ build/ui/",
    "start": "node scripts/start.js",
    "build": "mkdir -p build/ui build/dna; npm run hc:build && npm run ui:build",
    "test": "node scripts/test.js --env=jsdom",
    "hc:test-win": "npm run hc:build-win && if exist build\\test\\ cmd /c rd /S /Q build\\test && xcopy /s /e /y test build\\test\\ && cd build && hcdev test"
  },
  "author": "",
  "license": "GPL",
  "dependencies": {
    "@ckeditor/ckeditor5-build-inline": "^11.2.0",
    "@ckeditor/ckeditor5-react": "^1.1.0",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "bootstrap": "^4.1.3",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "i": "^0.3.6",
    "immutable": "^3.8.2",
    "jest": "22.4.2",
    "moment": "^2.22.2",
    "npm": "^6.4.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.2",
    "raf": "3.4.0",
    "react": "^16.6.3",
    "react-bootstrap": "^1.0.0-beta.3",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.6.3",
    "react-icons": "^3.2.2",
    "react-moment": "^0.8.4",
    "react-redux": "^5.1.1",
    "react-redux-loading-bar": "^4.1.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "react-tag-autocomplete": "^5.8.0",
    "react-tag-input": "^6.1.0",
    "react-virtualized": "^9.20.1",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "resolve": "1.6.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "^3.0.0",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.13",
    "babel-eslint": "^8.2.6",
    "cpx": "^1.5.0",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.11.1",
    "immutable": "^3.8.2",
    "typescript": "^2.9.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/ui-src/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/ui-src/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
